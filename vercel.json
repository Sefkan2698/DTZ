{
  "buildCommand": "npm run build --prod",
  "outputDirectory": "dist/DTZ",
  "installCommand": "npm ci",
  "framework": "angular",
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(js|css|webp|png|jpg|jpeg|svg|woff2|woff|ico))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}